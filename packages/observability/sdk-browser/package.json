{
  "name": "@dex-monit/observability-sdk-browser",
  "version": "1.0.1",
  "description": "Browser SDK for Dex Monitoring - Error tracking for React & Next.js apps",
  "keywords": [
    "observability",
    "monitoring",
    "react",
    "nextjs",
    "error-tracking",
    "web",
    "browser",
    "crash-reporting"
  ],
  "author": "Dex Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dex/dex-monit.git",
    "directory": "packages/observability/sdk-browser"
  },
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "require": "./dist/react/index.js",
      "default": "./dist/react/index.js"
    },
    "./next": {
      "types": "./dist/next/index.d.ts",
      "require": "./dist/next/index.js",
      "default": "./dist/next/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "nx": {
    "name": "sdk-browser"
  },
  "scripts": {
    "build": "tsc --build tsconfig.lib.json",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "next": {
      "optional": true
    }
  }
}
